<% provide(:title, 'new post') %>
<h1>Enter your json data</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@post) do |f| %>
    

      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :content %>
      <%= f.text_area :content %><br>
      

      <%= f.submit "store data", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
